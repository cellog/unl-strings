    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.2.0",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "fbjs": "^0.8.1",
    "jsdom": "^8.0.4",
    "lodash": "^4.11.2",
    "mocha": "2.x.x",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.0",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "domready": "^1.0.8",
